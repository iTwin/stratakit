{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"files": {
		"ignore": ["package.json"],
		"ignoreUnknown": true
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"overrides": [
		{
			"include": ["packages/kiwi-react/**"],
			"linter": {
				"rules": {
					"a11y": { "all": false }
				}
			}
		}
	]
}
