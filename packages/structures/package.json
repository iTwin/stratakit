{
	"name": "@stratakit/structures",
	"type": "module",
	"version": "0.3.1",
	"license": "MIT",
	"sideEffects": false,
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"@stratakit/source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"development": "./dist/DEV/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./unstable_AccordionItem": {
			"@stratakit/source": "./src/AccordionItem.tsx",
			"types": "./dist/AccordionItem.d.ts",
			"development": "./dist/DEV/AccordionItem.js",
			"default": "./dist/AccordionItem.js"
		},
		"./unstable_Banner": {
			"@stratakit/source": "./src/Banner.tsx",
			"types": "./dist/Banner.d.ts",
			"development": "./dist/DEV/Banner.js",
			"default": "./dist/Banner.js"
		},
		"./Chip": {
			"@stratakit/source": "./src/Chip.tsx",
			"types": "./dist/Chip.d.ts",
			"development": "./dist/DEV/Chip.js",
			"default": "./dist/Chip.js"
		},
		"./DropdownMenu": {
			"@stratakit/source": "./src/DropdownMenu.tsx",
			"types": "./dist/DropdownMenu.d.ts",
			"development": "./dist/DEV/DropdownMenu.js",
			"default": "./dist/DropdownMenu.js"
		},
		"./unstable_ErrorRegion": {
			"@stratakit/source": "./src/ErrorRegion.tsx",
			"types": "./dist/ErrorRegion.d.ts",
			"development": "./dist/DEV/ErrorRegion.js",
			"default": "./dist/ErrorRegion.js"
		},
		"./Table": {
			"@stratakit/source": "./src/Table.tsx",
			"types": "./dist/Table.d.ts",
			"development": "./dist/DEV/Table.js",
			"default": "./dist/Table.js"
		},
		"./Tabs": {
			"@stratakit/source": "./src/Tabs.tsx",
			"types": "./dist/Tabs.d.ts",
			"development": "./dist/DEV/Tabs.js",
			"default": "./dist/Tabs.js"
		},
		"./unstable_Toolbar": {
			"@stratakit/source": "./src/Toolbar.tsx",
			"types": "./dist/Toolbar.d.ts",
			"development": "./dist/DEV/Toolbar.js",
			"default": "./dist/Toolbar.js"
		},
		"./Tree": {
			"@stratakit/source": "./src/Tree.tsx",
			"types": "./dist/Tree.d.ts",
			"development": "./dist/DEV/Tree.js",
			"default": "./dist/Tree.js"
		},
		"./TreeItem": {
			"@stratakit/source": "./src/TreeItem.tsx",
			"types": "./dist/TreeItem.d.ts",
			"development": "./dist/DEV/TreeItem.js",
			"default": "./dist/TreeItem.js"
		}
	},
	"files": [
		"dist",
		"CHANGELOG.md",
		"LICENSE.md"
	],
	"description": "Medium-sized component structures for the Strata design system",
	"author": "Bentley Systems",
	"homepage": "https://github.com/iTwin/design-system",
	"repository": {
		"type": "git",
		"url": "https://github.com/iTwin/design-system.git",
		"directory": "packages/structures"
	},
	"keywords": [
		"component",
		"components",
		"design-system",
		"design",
		"frontend",
		"itwin",
		"itwinui",
		"strata",
		"stratakit",
		"react",
		"ui"
	],
	"scripts": {
		"build": "wireit",
		"dev": "wireit"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.17",
		"@stratakit/bricks": "^0.3.3",
		"classnames": "^2.5.1",
		"zustand": "^5.0.6"
	},
	"devDependencies": {
		"@stratakit/foundations": "workspace:*",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"esbuild": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"@stratakit/foundations": "^0.2.3",
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"wireit": {
		"build": {
			"dependencies": [
				"../foundations:build",
				"../bricks:build",
				"build:js",
				"build:DEV",
				"build:types"
			]
		},
		"build:js": {
			"command": "node scripts/build.js",
			"files": [
				"src",
				"scripts",
				"../../internal"
			],
			"output": [
				"dist/**/*.js",
				"!dist/DEV"
			]
		},
		"build:DEV": {
			"command": "node scripts/build.js",
			"files": [
				"src",
				"scripts",
				"../../internal"
			],
			"output": [
				"dist/DEV/**/*.js"
			],
			"env": {
				"NODE_ENV": "development"
			}
		},
		"build:types": {
			"command": "tsc --outDir dist",
			"dependencies": [
				"../foundations:build:types",
				"../bricks:build:types"
			],
			"files": [
				"src"
			],
			"output": [
				"dist/**/*.d.ts"
			]
		},
		"dev": {
			"command": "tsc --watch --outDir dist",
			"files": [
				"src"
			],
			"service": true
		}
	}
}
