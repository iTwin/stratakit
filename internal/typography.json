		"mono": {
			"$type": "fontFamily",
			"$value": "Geist Mono"
		}
	},
	"weight": {
		"black": {
			"$type": "fontWeight",
			"$value": 900
		},
		"bold": {
			"$type": "fontWeight",
			"$value": 700
