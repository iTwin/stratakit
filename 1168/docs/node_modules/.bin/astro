#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/design-system/design-system/node_modules/.pnpm/astro@5.16.9_@types+node@22.19.7_lightningcss@1.30.1_rollup@4.55.1_tsx@4.21.0_typescript@5.9.3_yaml@2.6.1/node_modules/astro/node_modules:/home/runner/work/design-system/design-system/node_modules/.pnpm/astro@5.16.9_@types+node@22.19.7_lightningcss@1.30.1_rollup@4.55.1_tsx@4.21.0_typescript@5.9.3_yaml@2.6.1/node_modules:/home/runner/work/design-system/design-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/design-system/design-system/node_modules/.pnpm/astro@5.16.9_@types+node@22.19.7_lightningcss@1.30.1_rollup@4.55.1_tsx@4.21.0_typescript@5.9.3_yaml@2.6.1/node_modules/astro/node_modules:/home/runner/work/design-system/design-system/node_modules/.pnpm/astro@5.16.9_@types+node@22.19.7_lightningcss@1.30.1_rollup@4.55.1_tsx@4.21.0_typescript@5.9.3_yaml@2.6.1/node_modules:/home/runner/work/design-system/design-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
