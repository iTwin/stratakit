{
  "name": "stratakit-docs",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "wireit",
    "test": "wireit",
    "preview": "astro preview",
    "format": "prettier --write . && biome format . --fix",
    "astro": "astro",
    "extract-api": "node --env-file-if-exists=.env api.ts"
  },
  "wireit": {
    "build": {
      "command": "astro build",
      "files": [
        "src/**/*",
        "public",
        "astro.config.mjs",
        "tsconfig.json",
        "api.json"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "pnpm exec playwright test",
      "files": [
        "tests/*.spec.js",
        "playwright.config.js"
      ],
      "dependencies": [
        "build"
      ],
      "output": [
        "tests/test-results"
      ]
    }
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/starlight": "^0.36.2",
    "@stratakit/bricks": "^0.5.0",
    "@stratakit/foundations": "^0.4.0",
    "@stratakit/icons": "^0.2.0",
    "@stratakit/structures": "^0.5.0",
    "astro": "^5.15.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "~2.2.7",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/xml2js": "^0.4.14",
    "axe-core": "^4.11.0",
    "prettier": "~3.6.2",
    "remark-directive": "^4.0.0",
    "ts-morph": "^27.0.2",
    "unist-util-visit": "^5.0.0",
    "wireit": "^0.14.12",
    "xml2js": "^0.6.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.18.0"
  }
}
